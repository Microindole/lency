// 方法调用测试
struct Rect {
    int w
    int h
}

impl Rect {
    // 面积计算 - 使用显式 this 访问
    int area() {
        return this.w * this.h 
    }
    
    // 设置宽度
    void setW(int newW) {
        this.w = newW 
    }

    // 获取高度
    int getH() {
        return this.h
    }
}

void main() {
    print("Testing Methods...")
    var r = Rect { w: 10, h: 20 }
    
    print(r.area())  // 200
    
    r.setW(5)
    print(r.area())  // 100
    
    print(r.getH())  // 20

    print("Methods test passed")
}

trait Display {
   void print_self();
}

// 泛型约束语法测试
// T 必须实现 Display trait
void print_it<T: Display>(T item) {
    // item.print_self(); 
}

struct Wrapper<T: Display> {
    T value
}

// impl 块的约束
impl<T: Display> Wrapper<T> {
    void display() {
        // this.value.print_self();
    }
}

struct MyInt {
    int val
}

impl Display for MyInt {
    void print_self() {
        print(this.val);
    }
}

void main() {
    var i = MyInt { val: 42 };
    print_it<MyInt>(i);
    
    var w = Wrapper<MyInt> { value: i };
    w.display();
}

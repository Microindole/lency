enum Res {
    Success(int),
    Failure
}

void main() {
    var a = Res.Success(100)
    var b = Res.Failure

    match a {
        case Res.Success(x) => print(x),
        case Res.Failure => print(0) // Failure
    }

    match b {
        case Res.Success(x) => print(1), // Success
        case Res.Failure => print(0) // Failure
    }
}

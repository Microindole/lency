int main() {
    // Nested array in expressions
    var a: [2]int = [1, 2]
    var b: [2]int = [3, 4]
    
    // Array in arithmetic expressions
    print(a[0] + b[0])  // 1 + 3 = 4
    print(a[1] * b[1])  // 2 * 4 = 8
    
    // Array with complex expressions as elements
    var c: [3]int = [10 + 5, 20 * 2, 30 - 10]
    print(c[0])  // 15
    print(c[1])  // 40
    print(c[2])  // 20
    
    return 0
}
